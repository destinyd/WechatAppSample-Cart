/**app.wxss**/
.container {
  position: absolute;
  width: 100%;
  top: 0;
  bottom: 0;
  overflow-y: scroll;
} 
.header{
  text-align: center;
  font-size: 1.2rem;
  font-weight: bold;
  /*border-bottom: 1px black solid;*/
  position: absolute;
  height: 50rpx;
  top: 0;
  width: 100%;
}
.layout-bottom{
    position: absolute;
    height: 50rpx;
    width: 100%;
    text-align: center;
    bottom: 0;
}
.btn{
    line-height: 50px;
    padding: 0 10px;
    height: 50px;
    cursor: pointer;
    color: #FFF;
    background-color: #1B1C1D;
}
.btn.primary{
    background-color: #FB4E44;
}
.layout-bottom .btn{
    float: right;
}
.navigator-hover {
  color:blue;
}
/** 自定义其他点击态样式类 **/
.other-navigator-hover {
  background-color: rgba(0, 0, 0, 0.1); opacity: 0.7;
  color:red;
}

.cart_icon{
  position: absolute;
  width: 100rpx;
  height: 100rpx;
  left: 50rpx;
  bottom: 50rpx;
  z-index: 999;
}